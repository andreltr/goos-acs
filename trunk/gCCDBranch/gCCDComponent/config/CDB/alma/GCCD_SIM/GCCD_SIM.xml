<?xml version="1.0" encoding="UTF-8"?>
<CCDComponent
	xmlns="urn:schemas-cosylab-com:CCDComponent:1.0"
	xmlns:baci="urn:schemas-cosylab-com:BACI:1.0"
	xmlns:cdb="urn:schemas-cosylab-com:CDB:1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<actualAirTemperature default_timer_trig="2.0" description="The approximate temperature of the CCD chip." format="%9.4f" units="Degrees Celsius" default_value="-9999.9"/>
	<actualCCDTemperature default_timer_trig="2.0" description="The approximate temperature of the CCD chip." format="%9.4f" units="Degrees Celsius" default_value="-9999.9"/>
	<commandedCCDTemperature default_timer_trig="2.0" description="The temperature the CCD is to be cooled down to." format="%9.4f" units="Degrees Celsius" default_value="25.0" min_value="-55.0" max_value="45.0"/>
	<cameraName default_timer_trig="2.0" description="Name of the used camera." format="%s" units="-" default_value="GCCD_SIM"/>
	<cameraModel default_timer_trig="2.0" description="Model of the used camera." format="%l" units="-" default_value="0"/>
</CCDComponent>
